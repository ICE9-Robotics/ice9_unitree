<launch>
    <arg name="map" default="false"/>

    <!-- Unitree -->
    <include file="$(find unitree_legged_real)/launch/real.launch" />

    <!-- SLAM -->
    <include file="$(find slam_planner)/launch/slam_planner_online.launch" />

    <!-- EMLID Reach -->
    <include file="$(find ice9_unitree)/launch/include/reach_ros.launch" />

    <!-- Nav sat transform -->
    <include file="$(find ice9_unitree)/launch/include/navsat_transform.launch" />

    <!-- RTK GPS odom matcher -->
    <include file="$(find ice9_unitree)/launch/include/matcher.launch" />

    <!-- Waypoints follower -->
    <include file="$(find ice9_unitree)/launch/include/follow_waypoints.launch" />

    <!-- Diagnostics -->
    <include file="$(find ice9_unitree)/launch/include/diagnostics.launch" />

    <!-- Map server -->
    <group if="$(arg map)">
        <node name="map_server" pkg="map_server" type="map_server" args="$(env HOME)/ice9_maps/map.yaml"/>
    </group>
</launch>
